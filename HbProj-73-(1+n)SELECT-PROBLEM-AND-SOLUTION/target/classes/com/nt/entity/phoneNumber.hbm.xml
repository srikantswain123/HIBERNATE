<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	<hibernate-mapping package="com.nt.entity">
	  <class name="PhoneNumber" table="PHONEDETAILS_HQL_JOINS">
	  <id name="regNo">
	   <generator class="sequence"/>
	  </id>
	  <property name="phoneNumber" type="long"/>
	  <property name="type" type="string" length="15"/>
	  <property name="provider" type="string" length="15"/>
	  <many-to-one name="user" class="UserInfo" column="User_Id" cascade="all" lazy="proxy" />
	  </class>
	</hibernate-mapping>