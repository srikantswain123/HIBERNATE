<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	<hibernate-mapping package="com.nt.entity">
	  <class name="UserInfo" table="USERINFOS_BAG">
	  <id name="userId">
	   <generator class="increment"/>
	  </id>
	  <property name="uname" type="string" length="15"/>
	  <property name="uaddrs" type="string" length="15"/>
	  <bag name="phoneDetails" cascade="all-delete-orphan" lazy="extra">
	    <key column="user_Id" />
	    <one-to-many class="PhoneNumber" />
	  </bag>
	  </class>
	</hibernate-mapping>